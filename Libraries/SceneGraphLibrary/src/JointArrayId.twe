class JointArrayId {
    JointArrayId (JointId root, TextString pattern) {
        this.root <- root;
        this.elementPattern <- pattern;
    }

    TextString getElementNamePattern() {
        return elementPattern;
    }

    JointId getRoot() {
        return root;
    }

    @CompletelyHidden JointId root;
    @CompletelyHidden TextString elementPattern;
}
