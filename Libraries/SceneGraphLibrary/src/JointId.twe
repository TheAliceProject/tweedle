class JointId {
    JointId (TextString name, JointId parent) {
        this.name <- name;
        this.parent <- parent;
    }

    TextString getName() {
        return this.name;
    }

    JointId getParent() {
        return this.parent;
    }

    @CompletelyHidden TextString name;
    @CompletelyHidden JointId parent;
}
